@model Ubrania_ASP.NET_Nowy.ViewModels.TicketViewModel
@{
    List<string> ImagesList = new List<string>();
    foreach (var byteArray in Model.Barcodes)
    {       
        var imgSrc = String.Format("data:image/gif;base64,{0}", byteArray);
        ImagesList.Add(imgSrc);
    }

}


<br />


@foreach (var image in ImagesList)
{
    <img src="@image" />
}


