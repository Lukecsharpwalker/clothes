@model Ubrania_ASP.NET_Nowy.ViewModels.TicketViewModel
@{
    List<string> ImagesList = new List<string>();
    foreach (var byteArray in Model.Barcodes)
    {
        var imgSrc = String.Format("data:image/gif;base64,{0}", byteArray);
        ImagesList.Add(imgSrc);
    }

    List<string> TicketList = new List<string>();
    foreach (var byteArray2 in Model.Ticket)
    {
        var base64 = Convert.ToBase64String(byteArray2);
        var imgSrc2 = String.Format("data:image/gif;base64,{0}", base64);
        TicketList.Add(imgSrc2);
    }
}


<br />


@for (int i = 0; i < ImagesList.Count; i++)
{
    <img src="@ImagesList[i]" />
    <img src="@TicketList[i]" />
}

